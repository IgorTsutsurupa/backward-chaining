mother(?x, ?y) -> woman(?x)
father(?x, ?y) -> man(?x)
mother(?x, ?y) -> parent(?x, ?y)
father(?x, ?y) -> parent(?x, ?y)
child(?x, ?y) -> parent(?y, ?x)
parent(?x, ?y) -> child(?x, ?y)
child(?x, ?y) & man(?x) -> son(?x, ?y)
child(?x, ?y) & woman(?x) -> daughter(?x, ?y)
son(?x, ?y) -> child(?x, ?y)
son(?x, ?y) -> man(?x)
daughter(?x, ?y) -> child(?x, ?y)
daughter(?x, ?y) -> woman(?x)
sibling(?x, ?y) & man(?x) -> brother(?x, ?y)
sibling(?x, ?y) & woman(?x) -> sister(?x, ?y)
brother(?x, ?y) -> sibling(?x, ?y)
brother(?x, ?y) -> man(?x)
sister(?x, ?y) -> sibling(?x, ?y)
sister(?x, ?y) -> woman(?x, ?y)
parent(?t, ?y) & brother(?x, ?t) -> uncle(?x, ?y)
parent(?t, ?y) & sister(?x, ?t) -> aunt(?x, ?y)
man(?x) & uncle(?y, ?x) -> nephew(?x, ?y)
man(?x) & aunt(?y, ?x) -> nephew(?x, ?y)
woman(?x) & uncle(?y, ?x) -> niece(?x, ?y)
woman(?x) & aunt(?y, ?x) -> niece(?x, ?y)
nephew(?x, ?y) -> man(?x)
niece(?x, ?y) -> woman(?x)
nephew(?y, ?x) & man(?x) -> uncle(?x, ?y)
niece(?y, ?x) & man(?x) -> uncle(?x, ?y)
nephew(?y, ?x) & woman(?x) -> aunt(?x, ?y)
niece(?y, ?x) & woman(?x) -> aunt(?x, ?y)